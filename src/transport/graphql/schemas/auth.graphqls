type TokenPair {
  accessToken: String!
  refreshToken: String!
}

type User {
  id: ID!
  email: String!
}

extend type Mutation {
  login(email: String!, password: String!): TokenPair!
  refresh(refreshToken: String): TokenPair!
  logout(refreshToken: String): Boolean!
}

extend type Query {
  me: String!
}
